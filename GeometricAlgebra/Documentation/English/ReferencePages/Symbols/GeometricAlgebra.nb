      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"{", "1", "}"}], "->", "\"\<x\>\""}], ",", 
        RowBox[{
         RowBox[{"{", "2", "}"}], "->", "\"\<y\>\""}]}], "}"}]}]}], "]"}]}], 
