Cell[BoxData[
 TemplateBox[{"3", "4", "5"},
  "Multivector",
  DisplayFunction->(RowBox[{
     RowBox[{#, "x"}], "+", 
     RowBox[{#2, "y"}], "+", 
     RowBox[{#3, 
       SubscriptBox["e", 
        RowBox[{3}]]}]}]& ),
  Editable->True,
  InterpretationFunction->(
   RowBox[{"Multivector", "[", "\"Coordinates\"", "->", "SparseArray", "[", 
     "{", 
     RowBox[{"2", "->", #}], ",", 
     RowBox[{"3", "->", #2}], ",", 
     RowBox[{"4", "->", #3}], "}", ",", "16", "]", ",", 
     RowBox[{"\"GeometricAlgebra\"", "\[Rule]", 
       InterpretationBox[
        SubscriptBox["\[DoubleStruckCapitalG]", 
         RowBox[{"3", ",", "1"}]], 
        Wolfram`GeometricAlgebra`GeometricAlgebra[
        "Signature" -> {3, 1, 0}, "FormatIndex" -> {{1} -> "x", {2} -> "y"}], 
        Tooltip -> "Geometric Algebra"]}], "]"}]& ),
  Tooltip->RowBox[{"Multivector", " ", 
     InterpretationBox[
      SubscriptBox["\[DoubleStruckCapitalG]", 
       RowBox[{"3", ",", "1"}]], 
      Wolfram`GeometricAlgebra`GeometricAlgebra[
      "Signature" -> {3, 1, 0}, "FormatIndex" -> {{1} -> "x", {2} -> "y"}], 
      Tooltip -> "Geometric Algebra"]}]]], "Output",
